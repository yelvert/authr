{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "allowJs": false,
    "jsx": "react-jsx",
    "strict": true,
    "noImplicitAny": false,
    "declaration": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "paths": {
      "@admin/*": ["./app/frontend/admin/*"],
      "@app/*": ["./app/frontend/app/*"],
      "@forward_auth/*": ["./app/frontend/forward_auth/*"],
      "@sessions/*": ["./app/frontend/sessions/*"],
      "@shared/*": ["./app/frontend/shared/*"],
      "@sdk": ["./app/frontend/sdk/index"],
      "@/*": ["./app/frontend/*"],
      "@*": ["./app/frontend"],
    }
  },
  "exclude": [
    "**/dist",
    "**/node_modules",
    "**/public"
  ]
}